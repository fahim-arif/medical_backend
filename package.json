{
  "name": "medical_backend",
  "version": "1.0.0",
  "main": "index.js",
  "license": "MIT",
  "scripts": {
    "dev": "nodemon --config nodemon.json",
    "build": "tsc",
    "start": "tsc && node dist/server.js",
    "ts-node-dev": "ts-node-dev --respawn --pretty --transpile-only src/server.ts",
    "lint": "eslint . --ext .ts",
    "lint:fix": "eslint . --ext .ts --fix"
  },
  "_moduleAliases": {
    "@middlewares": "dist/middlewares",
    "@models": "dist/models",
    "@repositories": "dist/repositories",
    "@routes": "dist/routes",
    "@services": "dist/services",
    "@utils": "dist/utils",
    "@external": "dist/external"
  },
  "dependencies": {
    "@prisma/client": "^3.6.0",
    "axios": "^0.24.0",
    "cors": "^2.8.5",
    "dotenv": "^10.0.0",
    "express": "^4.17.1",
    "express-validation": "^3.0.8",
    "helmet": "^4.6.0",
    "module-alias": "^2.2.2",
    "morgan": "^1.10.0",
    "nanoid": "^3.1.30",
    "passport": "^0.5.0",
    "prisma": "^3.6.0",
    "winston": "^3.3.3"
  },
  "devDependencies": {
    "@types/cors": "^2.8.12",
    "@types/dotenv": "^8.2.0",
    "@types/express": "^4.17.13",
    "@types/express-jwt": "^6.0.3",
    "@types/module-alias": "^2.0.1",
    "@types/morgan": "^1.9.3",
    "@types/node": "^16.11.12",
    "@typescript-eslint/eslint-plugin": "5.6.0",
    "@typescript-eslint/parser": "5.6.0",
    "eslint": "8.4.1",
    "eslint-config-airbnb": "19.0.2",
    "nodemon": "^2.0.15",
    "prettier": "^2.5.1",
    "ts-node": "^10.4.0",
    "ts-node-dev": "^1.1.8",
    "tslint": "^6.1.3"
  },
  "author": "Fahim Arif",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/fahim-arif/medical-backend.git"
  }
